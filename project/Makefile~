CPPFLAGS = -I/usr/include/irrlicht
CXXFLAGS = -Wall -Wextra -O2 -g -std=c++11 -Wno-unused-parameter

irrlicht-gui: main.o events.o characters.o menu.o player.o scene.o
	g++ $^ -o $@ -lIrrlicht

main.o events.o characters.o menu.o player.o scene.o: events.h gui_ids.h characters.h menu.h player.h scene.h
clean:
	@rm -f irrlicht-gui *.o
